@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gxechocat#echocat>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxechocat.html> ;
        modgui:stylesheet <modgui/stylesheet-gxechocat.css> ;
        modgui:screenshot <modgui/screenshot-gxechocat.png> ;
        modgui:thumbnail <modgui/thumbnail-gxechocat.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "Echo Cat" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "slime" ;
        modgui:knob "blue" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "input_gain" ;
            lv2:name "Input Gain" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "swell" ;
            lv2:name "Swell" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "sustain" ;
            lv2:name "Sustain" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "outputgain" ;
            lv2:name "Level" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "head1" ;
            lv2:name "Head 1" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "head2" ;
            lv2:name "Head 2" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "head3" ;
            lv2:name "Head 3" ;
        ] ;
    ] .