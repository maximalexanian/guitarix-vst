<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:guitarix-developer@lists.sourceforge.net> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gx_hornet_>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "Gx_hornet_" .

<http://guitarix.sourceforge.net/plugins/gx_hornet_#_hornet_>
    a lv2:Plugin ,
        lv2:DistortionPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "GxHornet";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gx_hornet_> ;
    lv2:optionalFeature lv2:hardRTCapable ;
      
    lv2:minorVersion 34;
    lv2:microVersion 0;

  guiext:ui <http://guitarix.sourceforge.net/plugins/gx_hornet_gui#_hornet_>;
    
rdfs:comment """

...

""";

    lv2:port  [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 0 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 1 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] 
      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "FUZZ" ;
        lv2:name "FUZZ" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "SUSTAIN" ;
        lv2:name "SUSTAIN" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "VOLUME" ;
        lv2:name "VOLUME" ;
        lv2:default 0.5 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] .

<http://guitarix.sourceforge.net/plugins/gx_hornet_gui#_hornet_>
  a guiext:GtkUI;
  guiext:binary <gx_hornet_gui.so>;
  guiext:requiredFeature guiext:makeResident;
  .
