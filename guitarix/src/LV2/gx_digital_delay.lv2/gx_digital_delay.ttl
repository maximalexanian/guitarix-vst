<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:guitarix-developer@lists.sourceforge.net> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gx_digital_delay_>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "Gx_digital_delay_" .

<http://guitarix.sourceforge.net/plugins/gx_digital_delay_#_digital_delay_>
    a lv2:Plugin ,
        lv2:DelayPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "Gxdigital_delay";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gx_digital_delay_> ;
    lv2:requiredFeature urid:map ;
    lv2:optionalFeature lv2:hardRTCapable ;

  guiext:ui <http://guitarix.sourceforge.net/plugins/gx_digital_delay_#gui>;
      
    lv2:minorVersion 34;
    lv2:microVersion 0;
    
rdfs:comment """

...

""";

    lv2:port  [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 0 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 1 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] 
      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "BPM" ;
        lv2:name "BPM" ;
        lv2:default 1.2e+02 ;
        lv2:minimum 24.0 ;
        lv2:maximum 3.6e+02 ;
        lv2:portProperty log:notOnGUI ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "FEEDBACK" ;
        lv2:name "FEEDBACK" ;
        lv2:default 5e+01 ;
        lv2:minimum 1.0 ;
        lv2:maximum 1e+02 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "GAIN" ;
        lv2:name "GAIN" ;
        lv2:default 1e+02 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.2e+02 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:portProperty log:logarithmic ;
        lv2:symbol "HIGHPASS" ;
        lv2:name "HIGHPASS" ;
        lv2:default 1.2e+02 ;
        lv2:minimum 2e+01 ;
        lv2:maximum 2e+04 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:portProperty log:logarithmic ;
        lv2:symbol "HOWPASS" ;
        lv2:name "HOWPASS" ;
        lv2:default 1.2e+04 ;
        lv2:minimum 2e+01 ;
        lv2:maximum 2e+04 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "LEVEL" ;
        lv2:name "LEVEL" ;
        lv2:default 5e+01 ;
        lv2:minimum 1.0 ;
        lv2:maximum 1e+02 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "MODE" ;
        lv2:name "MODE" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 3.0 ;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ; 
        lv2:scalePoint [rdfs:label "plain"; rdf:value 0];
        lv2:scalePoint [rdfs:label "presence"; rdf:value 1];
        lv2:scalePoint [rdfs:label "tape"; rdf:value 2];
        lv2:scalePoint [rdfs:label "tape2"; rdf:value 3];

    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "NOTES" ;
        lv2:name "NOTES" ;
        lv2:default 5.0 ;
        lv2:minimum 1.0 ;
        lv2:maximum 18.0 ;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ; 
        lv2:scalePoint [rdfs:label "Dotted1/2note"; rdf:value 1];
        lv2:scalePoint [rdfs:label "1/2note"; rdf:value 2];
        lv2:scalePoint [rdfs:label "1/2notetriplets"; rdf:value 3];
        lv2:scalePoint [rdfs:label "Dotted1/4note"; rdf:value 4];
        lv2:scalePoint [rdfs:label "1/4note"; rdf:value 5];
        lv2:scalePoint [rdfs:label "1/4notetriplets"; rdf:value 6];
        lv2:scalePoint [rdfs:label "Dotted1/8note"; rdf:value 7];
        lv2:scalePoint [rdfs:label "1/8note"; rdf:value 8];
        lv2:scalePoint [rdfs:label "1/8notetriplets"; rdf:value 9];
        lv2:scalePoint [rdfs:label "Dotted1/16note"; rdf:value 10];
        lv2:scalePoint [rdfs:label "1/16note"; rdf:value 11];
        lv2:scalePoint [rdfs:label "1/16notetriplets"; rdf:value 12];
        lv2:scalePoint [rdfs:label "Dotted1/32note"; rdf:value 13];
        lv2:scalePoint [rdfs:label "1/32note"; rdf:value 14];
        lv2:scalePoint [rdfs:label "1/32notetriplets"; rdf:value 15];
        lv2:scalePoint [rdfs:label "Dotted1/64note"; rdf:value 16];
        lv2:scalePoint [rdfs:label "1/64note"; rdf:value 17];
        lv2:scalePoint [rdfs:label "1/64notetriplets"; rdf:value 18];

    ] , [
        a lv2:InputPort ,
            atom:AtomPort ;
        atom:bufferType atom:Sequence ;
        atom:supports time:Position ;
        lv2:index 10 ;
        lv2:symbol "DD_CONTROL" ;
        lv2:name "DD_CONTROL" ;
    ] , [
        a lv2:OutputPort ,
            lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "DD_NOTIFY" ;
        lv2:name "DD_NOTIFY";
        lv2:portProperty lv2:integer;
        lv2:default 120.0 ;
        lv2:minimum 24.0 ;
        lv2:maximum 360.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 12 ;
        lv2:symbol "SYNC" ;
        lv2:name "SYNC" ;
        lv2:portProperty lv2:integer,
          lv2:toggled;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 13 ;
        lv2:symbol "HOSTBPM" ;
        lv2:name "HOSTBPM" ;
        lv2:default 1.2e+02 ;
        lv2:minimum 24.0 ;
        lv2:maximum 3.6e+02 ;
        lv2:designation time:beatsPerMinute;
    ] .

