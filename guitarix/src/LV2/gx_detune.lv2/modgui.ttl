@prefix modgui: <http://moddevices.com/ns/modgui#> .
@prefix lv2:    <http://lv2plug.in/ns/lv2core#> .

<http://guitarix.sourceforge.net/plugins/gx_detune_#_detune_>
    modgui:gui [
        modgui:resourcesDirectory <modgui> ;
        modgui:iconTemplate <modgui/icon-gxdetune.html> ;
        modgui:stylesheet <modgui/stylesheet-gxdetune.css> ;
        modgui:screenshot <modgui/screenshot-gxdetune.png> ;
        modgui:thumbnail <modgui/thumbnail-gxdetune.png> ;
        modgui:brand "Guitarix" ;
        modgui:label "Detune" ;
        modgui:model "boxy" ;
        modgui:panel "7-knobs" ;
        modgui:color "lava" ;
        modgui:knob "green" ;
        modgui:port [
            lv2:index 0 ;
            lv2:symbol "DETUNE" ;
            lv2:name "DETUNE" ;
        ] , [
            lv2:index 1 ;
            lv2:symbol "OCTAVE" ;
            lv2:name "OCTAVE" ;
        ] , [
            lv2:index 2 ;
            lv2:symbol "DRY" ;
            lv2:name "DRY" ;
        ] , [
            lv2:index 3 ;
            lv2:symbol "WET" ;
            lv2:name "WET" ;
        ] , [
            lv2:index 4 ;
            lv2:symbol "LOW" ;
            lv2:name "LOW" ;
        ] , [
            lv2:index 5 ;
            lv2:symbol "MIDDLETREBLE" ;
            lv2:name "MID" ;
        ] , [
            lv2:index 6 ;
            lv2:symbol "TREBLE" ;
            lv2:name "TREBLE" ;
        ] ;
    ] .