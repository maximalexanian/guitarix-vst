@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.
@prefix time: <http://lv2plug.in/ns/ext/time#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#> .
@prefix bufsz: <http://lv2plug.in/ns/ext/buf-size#> .
@prefix atom:  <http://lv2plug.in/ns/ext/atom#> .
@prefix urid:  <http://lv2plug.in/ns/ext/urid#> .
@prefix opt:  <http://lv2plug.in/ns/ext/options#> .
@prefix work:  <http://lv2plug.in/ns/ext/worker#> .

<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:guitarix-developer@lists.sourceforge.net> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gx_detune_>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "Gx_detune_" .

<http://guitarix.sourceforge.net/plugins/gx_detune_#_detune_>
    a lv2:Plugin ,
        lv2:PitchPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "Gxdetune";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gx_detune_> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    lv2:requiredFeature urid:map ,
        bufsz:boundedBlockLength ,
        work:schedule ;
    bufsz:minBlockLength 64 ;
    bufsz:maxBlockLength 8192 ;
    lv2:extensionData work:interface ;

  guiext:ui <http://guitarix.sourceforge.net/plugins/gx_detune_#gui>;

      
    lv2:minorVersion 34;
    lv2:microVersion 0;
    
rdfs:comment """

...

""";

    lv2:port  [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 0 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 1 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] 
      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "DETUNE" ;
        lv2:name "DETUNE" ;
        lv2:default 0.0 ;
        lv2:minimum -12.0 ;
        lv2:maximum 12.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "OCTAVE" ;
        lv2:name "OCTAVE" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0 ;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ; 
        lv2:scalePoint [rdfs:label "normal"; rdf:value 0];
        lv2:scalePoint [rdfs:label "octaveup"; rdf:value 1];
        lv2:scalePoint [rdfs:label "octavedown"; rdf:value 2];

    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "COMPENSATE" ;
        lv2:name "COMPENSATE" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 1.0 ;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ; 
        lv2:scalePoint [rdfs:label "latency"; rdf:value 0];
        lv2:scalePoint [rdfs:label "compensate"; rdf:value 1];

    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 5 ;
        lv2:symbol "LATENCY" ;
        lv2:name "LATENCY" ;
        lv2:default 0.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0 ;
        lv2:portProperty lv2:integer;
        lv2:portProperty lv2:enumeration ; 
        lv2:scalePoint [rdfs:label "highquality"; rdf:value 0];
        lv2:scalePoint [rdfs:label "lowquality"; rdf:value 1];
        lv2:scalePoint [rdfs:label "realtime"; rdf:value 2];

    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 6 ;
        lv2:symbol "WET" ;
        lv2:name "WET" ;
        lv2:default 50.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 7 ;
        lv2:symbol "DRY" ;
        lv2:name "DRY" ;
        lv2:default 50.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 100.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 8 ;
        lv2:symbol "LOW" ;
        lv2:name "LOW" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 9 ;
        lv2:symbol "MIDDLELOW" ;
        lv2:name "MIDDLELOW" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 10 ;
        lv2:symbol "MIDDLETREBLE" ;
        lv2:name "MIDDLETREBLE" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0 ;
    ]      , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 11 ;
        lv2:symbol "TREBLE" ;
        lv2:name "TREBLE" ;
        lv2:default 1.0 ;
        lv2:minimum 0.0 ;
        lv2:maximum 2.0 ;
    ], [
        a lv2:OutputPort,
        lv2:ControlPort ;
        lv2:designation <http://lv2plug.in/ns/lv2core#latency>;
        lv2:name "latency (samples)" ;
        lv2:index 12 ;
        lv2:symbol "latency" ;
        lv2:minimum 0 ;
        lv2:maximum 4096 ;
    ] .

<http://guitarix.sourceforge.net/plugins/gx_detune_#gui>
  a guiext:GtkUI;
  guiext:binary <gx_detune_gui.so>;
  guiext:requiredFeature guiext:makeResident .

