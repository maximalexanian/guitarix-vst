@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix lv2: <http://lv2plug.in/ns/lv2core#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix guiext: <http://lv2plug.in/ns/extensions/ui#>.

<http://guitarix.sourceforge.net#me>
	a foaf:Person ;
	foaf:name "Guitarix team" ;
	foaf:mbox <mailto:brummer@web.de> ;
	rdfs:seeAlso <http://guitarix.sourceforge.net> .

<http://guitarix.sourceforge.net/plugins/gxbooster>
	a doap:Project ;
	doap:maintainer <http://guitarix.sourceforge.net#me> ;
	doap:name "Gxbooster" .

<http://guitarix.sourceforge.net/plugins/gxbooster#booster>
    a lv2:Plugin ,
        lv2:UtilityPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "GxBooster";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gxbooster> ;
    lv2:optionalFeature lv2:hardRTCapable ;
    
  guiext:ui <http://guitarix.sourceforge.net/plugins/gxbooster#gui>;
  
    lv2:minorVersion 34;
    lv2:microVersion 0;
    
rdfs:comment """

A 2 band boost plugin. With this plugin you can boost the high and the low frequencies independently.

*Unofficial documentation

""";

    lv2:port [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "fslider0_" ;
        lv2:name "H Level";
        lv2:default 1.0 ;
        lv2:minimum 0.5 ;
        lv2:maximum 20.0 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "fslider1_" ;
        lv2:name "B Level";
        lv2:default 1.0 ;
        lv2:minimum 0.5 ;
        lv2:maximum 20.0 ;
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 2 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 3 ;
        lv2:symbol "in" ;
        lv2:name "in"
    ] .


<http://guitarix.sourceforge.net/plugins/gxbooster#gui>
  a guiext:GtkUI;
  guiext:binary <gxbooster_gui.so>;
  guiext:requiredFeature guiext:makeResident .
  
