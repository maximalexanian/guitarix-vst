<http://guitarix.sourceforge.net/plugins/gx_vibe_#_vibe_mono>
    a lv2:Plugin ,
        lv2:ModulatorPlugin ;
    doap:maintainer <http://guitarix.sourceforge.net#me> ;
    doap:name "GxvibeMono";
    doap:license <http://opensource.org/licenses/isc> ;
    lv2:project <http://guitarix.sourceforge.net/plugins/gx_vibe_> ;
    lv2:optionalFeature lv2:hardRTCapable ;
      
    lv2:minorVersion 34;
    lv2:microVersion 0;

  guiext:ui <http://guitarix.sourceforge.net/plugins/gx_vibe_gui#_vibe_mono>;
    
rdfs:comment """

...

""";

    lv2:port  [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 0 ;
        lv2:symbol "WIDTH" ;
        lv2:name "WIDTH" ;
        lv2:default 0.5 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 1 ;
        lv2:symbol "DEPTH" ;
        lv2:name "DEPTH" ;
        lv2:default 0.37 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 2 ;
        lv2:symbol "WETDRY" ;
        lv2:name "WETDRY" ;
        lv2:default 1 ;
        lv2:minimum 0 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 3 ;
        lv2:symbol "FB" ;
        lv2:name "FB" ;
        lv2:default -0.6 ;
        lv2:minimum -1 ;
        lv2:maximum 1 ;
    ] , [
        a lv2:InputPort ,
            lv2:ControlPort ;
        lv2:index 4 ;
        lv2:symbol "TEMPO" ;
        lv2:name "TEMPO" ;
        lv2:default 4.4 ;
        lv2:minimum 0.1 ;
        lv2:maximum 1e+01 ;
    ] , [
        a lv2:AudioPort ,
            lv2:OutputPort ;
        lv2:index 5 ;
        lv2:symbol "out" ;
        lv2:name "Out"
    ] , [
        a lv2:AudioPort ,
            lv2:InputPort ;
        lv2:index 6 ;
        lv2:symbol "in" ;
        lv2:name "In" ;
    ] .

